@model SGE.Models.Calificacion

@{
    ViewData["Title"] = "Details";
}

<h1>Ver detalles</h1>


<div>
    <h4>Calificacion</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Materia)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Materia.Nombre)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Alumno)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Alumno.Nombre)
        </dd>
        <dt class = "col-sm-2">
           Calificación@*  @Html.DisplayNameFor(model => model.Valor) *@
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Valor)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.CalificacionId" class="btn btn-info">Modificar</a> |
    <a asp-action="Index" class="btn btn-light">Regresar</a>
</div>

@model SGE.Models.Materia

@{
    ViewData["Title"] = "Details";
}

<h1>Ver detalles</h1>


<div>
    <h4>Materia</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Matería@* @Html.DisplayNameFor(model => model.Nombre) *@
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
        <dt class = "col-sm-2">
           Profesor@*  @Html.DisplayNameFor(model => model.Profesor) *@
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Profesor.Nombre)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.MateriaId" class="btn btn-info">Modificar</a> |
    <a asp-action="Index" class="btn btn-light">Regresar</a>
</div>
